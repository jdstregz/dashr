{
  "name": "dashr",
  "version": "1.0.0",
  "description": "A dashboard-view web app template for quick prototyping",
  "main": "app/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app/server.js",
    "debug": "nodemon app.server.js",
    "lint": "eslint . --fix",
    "coverage": "nyc nyc --reporter=html --reporter=lcov --reporter=text-summary npm run test",
    "view-coverage": "opener ./coverage/index.html",
    "client": "npm run start --prefix client",
    "both": "concurrently \"npm run debug\" \"npm run client\"",
    "build": "npm install && cd client && npm install && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "exclude": [
      "app/config/**"
    ]
  },
  "lint-staged": {
    "./**/*.{js, jsx}": [
      "eslint --fix",
      "git add",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdstregz/dashr.git"
  },
  "author": "Joshua Streger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdstregz/dashr/issues"
  },
  "homepage": "https://github.com/jdstregz/dashr#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.10",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^10.1.0",
    "nyc": "^14.1.1",
    "opener": "^1.5.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "request": "^2.88.0",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0"
  }
}
